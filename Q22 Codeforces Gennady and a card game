#include<bits/stdc++.h>
using namespace std;
 
int main(){
    string s;
    cin>>s;
    vector<string>v;
    for(int i=0;i<5;i++){
        string c;
        cin>>c;
        v.push_back(c);
    }
    int i;
    for(i=0;i<5;i++){
        string a = v[i];
        if(s[0]==a[0] || s[1]==a[1]){
            cout<<"YES";
            break;
        }
    }
    if(i==5){
        cout<<"NO";
    }
}
